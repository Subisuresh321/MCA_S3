    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Generate Bill"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="619dp" />

